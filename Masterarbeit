<!DOCTYPE html>
<html lang="de">
<head>
  <meta charset="UTF-8">
  <title>Weiterleitung…</title>

  <script>
    document.addEventListener("DOMContentLoaded", function () {
      const links = [
        "https://www.youtube.com/watch?v=hcnEfb9Qoc4",
        "https://www.youtube.com/watch?v=IhAXXY4SqW0"
      ];

      const randomLink = links[Math.floor(Math.random() * links.length)];
      window.location.replace(randomLink);
    });
  </script>
</head>

<body>
  <p>Bitte warten…</p>
</body>
</html>
